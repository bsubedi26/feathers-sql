{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "sqlite3": {
    "client": "sqlite3",
    "connection": {
      "filename": "./db.sqlite"
    }
  },
  "postgres": {
    "client": "pg",
    "connection": {
      "host": "localhost",
      "user": "postgres",
      "password": "root",
      "database": "greatbay_db"
    }
  },
  "mysql": {
    "client": "mysql2",
    "connection": {
      "host": "127.0.0.1",
      "user": "root",
      "password": "root",
      "database": "greatbay_db"
    }
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "user",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}